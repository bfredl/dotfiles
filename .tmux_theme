# This tmux statusbar config was created by tmuxline.vim
# on Wed, 05 Nov 2014

set -g status-bg "colour7"
set -g message-command-fg "colour253"
set -g status-justify "left"
set -g status-left-length "100"
set -g status "on"
set -g pane-active-border-fg "colour102"
set -g message-bg "colour239"
set -g status-right-length "100"
set -g status-right-attr "none"
set -g message-fg "colour253"
set -g message-command-bg "colour239"
set -g status-attr "none"
set -g status-utf8 "on"
set -g pane-border-fg "colour239"
set -g status-left-attr "none"
setw -g window-status-fg "colour102"
setw -g window-status-attr "none"
setw -g window-status-activity-bg "colour7"
setw -g window-status-activity-attr "none"
setw -g window-status-activity-fg "colour102"
setw -g window-status-separator ""
setw -g window-status-bg "colour7"
set -g status-left "#[fg=colour232,bg=colour239] #S #[fg=colour239,bg=colour7,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=colour236,bg=colour7,nobold,nounderscore,noitalics]#[fg=colour102,bg=colour236] %Y-%m-%d  %H:%M #[fg=colour239,bg=colour236,nobold,nounderscore,noitalics]#[fg=colour232,bg=colour239] #h "
setw -g window-status-format "#[fg=colour232,bg=colour7] #I #[fg=colour232,bg=colour7] #W "
setw -g window-status-current-format "#[fg=colour7,bg=colour236,nobold,nounderscore,noitalics]#[fg=colour102,bg=colour236] #I  #W #[fg=colour236,bg=colour7,nobold,nounderscore,noitalics]"
